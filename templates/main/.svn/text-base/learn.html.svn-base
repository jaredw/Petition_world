{% extends "base.html" %}
{% block includes %}
    <link type="text/css" href="/css/learn.css" rel="stylesheet" />
    <script src="/js/learn.js" type="text/javascript"></script>
    <script src="http://earth.google.com/javascript/earth.js" type="text/javascript" ></script>
    <script src="http://earth.google.com/javascript/cache.js" type="text/javascript" ></script>
    <script src="http://earth.google.com/javascript/support.js" type="text/javascript" ></script>
    <script src="http://earth.google.com/javascript/display.js" type="text/javascript" ></script>
    <script src="/js/kmldomwalk.js" type="text/javascript"></script>
    <script src="/js/tours.js" type="text/javascript"></script>
{% endblock %}
{% block tabs %}
    <li class="ui-state-default ui-corner-top ui-tabs-selected ui-state-active"><a href="/learn">Learn</a></li>
    <li class="ui-state-default ui-corner-top"><a href="/vote">Vote</a></li>
    <li class="ui-state-default ui-corner-top"><a href="/explore">Explore</a></li>
{% endblock %}
{% block content %}
  <div id="learn">
    <div id="earth_container">
      <div id="tourlist_status">Loading...</div>
      <div id="tourlist_container">
        <ul id="tourlist">
        </ul>
      </div>
      <div id="map3d_container">
        <div id="map3d"></div>
      </div>
      <div id="caption">captions</div>
    </div>
  </div>
{% endblock %}
